    <system:String x:Key="NoAccess">无法访问游戏进程</system:String>
    <system:String x:Key="Hwnd">句柄</system:String>
    <system:String x:Key="Proc">进程</system:String>
    <system:String x:Key="Base">基址</system:String>
    <system:String x:Key="NoProcess">未发现有效游戏进程</system:String>
    <system:String x:Key="Description">说明</system:String>
    <system:String x:Key="Data">数据</system:String>
